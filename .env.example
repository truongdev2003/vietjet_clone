# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Database
MONGODB_URI=mongodb://admin:admin123@mongodb:27017/vietjet_clone?authSource=admin

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-refresh-secret-key
JWT_EXPIRE=24h
JWT_REFRESH_EXPIRE=30d

# Session
SESSION_SECRET=your-session-secret

# Email Configuration (Gmail example)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USERNAME=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_FROM=noreply@vietjet.com

# CORS
CORS_ORIGIN=http://localhost:3000

# Payment Gateway (Optional)
MOMO_PARTNER_CODE=
MOMO_ACCESS_KEY=
MOMO_SECRET_KEY=
MOMO_REDIRECT_URL=http://localhost:3000/payment/callback
MOMO_IPN_URL=http://localhost:5000/api/payment/momo/ipn

VNPAY_TMN_CODE=
VNPAY_HASH_SECRET=
VNPAY_URL=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html
VNPAY_RETURN_URL=http://localhost:3000/payment/callback
